        {
            "@type": "WorkflowStep",
            "name": "find_device",
            "description": null,
            "arguments": {
                "query": {
                    "sort": [],
                    "limit": 30,
                    "logic": "AND",
                    "filters": [
                        {
                            "type": "primitive",
                            "field": "id",
                            "value": "{{vars.input.params['device_record_id']|int}}",
                            "operator": "eq",
                            "_operator": "eq"
                        }
                    ],
                    "__selectFields": [
                        "connectorConfig",
                        "deviceName",
                        "aDOM",
                        "vDOM",
                        "sn",
                        "platform",
                        "firmware",
                        "connectionStatus",
                        "managementMode",
                        "configStatus",
                        "configDBStatus",
                        "sourceData",
                        "deviceIP",
                        "deviceMetadata",
                        "hostName"
                    ]
                },
                "module": "devices?$limit=1",
                "checkboxFields": true,
                "step_variables": []
            },
            "status": null,
